{
  "discord": {
    "command_prefix": "!",
    "opus_library_path": "/opt/homebrew/lib/libopus.dylib"
  },
  "spotify": {
    "device_name": "DiscordBot",
    "cache_directory": ".librespot-cache",
    "scopes": "user-modify-playback-state user-read-playback-state"
  },
  "ollama": {
    "api_url": "http://localhost:11434/api/chat",
    "model": "deepseek-r1:14b",
    "timeout": 60.0
  },
  "audio": {
    "local_files_directory": "./sound/"
  },
  "lavalink": {
    "host": "localhost",
    "port": 2333
  }
}
